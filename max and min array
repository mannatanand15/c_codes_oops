#include<stdio.h>
int main(){

    //DECLARING THE VARIABLE 
    
    int n,i;
    //taking input of size
    
    scanf("%d",&n);
    int arr[100];
    
    //taking inputof array
    
    for (i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int max=arr[0];
    int min=arr[0];
    
    //FOR MAXIMUM CASE
    
    for (i=1;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }    
        if(arr[i]<min){
            min=arr[i];
        }
    }
    
    printf("Maximum element is: %d\n",max);
    printf("Minimum element is: %d\n",min);
    return 0;
}
